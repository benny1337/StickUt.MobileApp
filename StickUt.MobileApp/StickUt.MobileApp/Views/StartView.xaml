<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StickUt.MobileApp.Views.StartView"
             Title="Stickut"
  
    >
  <StackLayout>
    <Button Command="{Binding buttoncmd}" Text="Ladda från api" />

    <ListView ItemsSource="{Binding Workouts}">
      <ListView.ItemTemplate>
      <DataTemplate>
        <ViewCell>
          <ViewCell.View>
            <StackLayout>
              <Label Text="här hade det kunnat stå något" />
              <Label Text="{Binding WorkoutStatus}" />
            </StackLayout>            
          </ViewCell.View>
        </ViewCell>
      </DataTemplate>
    </ListView.ItemTemplate>
    </ListView>
  </StackLayout>
</ContentPage>